{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Webrtc Chrome Extension",
  "description": "Webrtc Chrome Extension",
  "action": {
    "default_popup": "js/index.html",
    "default_title": "Webrtc Chrome Extension"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "permissions": ["activeTab", "storage"]
}
